#include <stdio.h>

int main() {

    int n, m, sum = 0;



    scanf("%d %d", &n, &m);



    if (n <= 0 || m <= 0 || n >= 30 || m >= 30 || n >= m) {

        printf("Invalid Input\n");

        return 0;

    }

    

    for (int i = n; i <= m; i++) {

        if (i % 2 == 0) {

            sum += i;

        }

    }

    

    printf("%d\n", sum);

    return 0;

}
