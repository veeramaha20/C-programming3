#include <stdio.h>

int main() {
    int n, sum = 0;
    scanf("%d", &n);

    if (n >= 100) {
        int temp = n;
        while (temp > 0) {
            sum += temp % 10;
            temp /= 10;
        }
        printf("Sum of digit is %d\n", sum);
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
